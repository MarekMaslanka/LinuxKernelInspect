{
	"name": "deku.kernel.inspector",
	"description": "Kernel source inspection tool",
	"version": "0.0.1",
	"publisher": "marek.maslanka",
	"private": true,
	"license": "MIT",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:c"
	],
	"main": "./out/extension.js",
	"contributes": {
		"capabilities": {
			"hoverProvider": "true"
		},
		"commands": [
			{
				"command": "outliner.outline",
				"title": "Outline"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "inspect-explorer",
					"title": "Inspects Explorer",
					"icon": "$(symbol-keyword)"
				}
			]
		},
		"views": {
			"inspect-explorer": [
				{
					"id": "documentOutline",
					"name": "File Inspects"
				},
				{
					"id": "functionReturnsTreeProvider",
					"name": "Function returns"
				},
				{
					"id": "functionStacktraceTreeProvider",
					"name": "Function stacktraces"
				},
				{
					"id": "documentOutline2",
					"name": "Inspects"
				},
				{
					"type": "webview",
					"id": "kernelInspect-sqlSidebar",
					"name": "SQL",
					"icon": "database",
					"contextualTitle": "SQL"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "inspect-explorer",
				"contents": "Welcome"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "16.18.1",
		"@types/vscode": "1.52.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"@vscode/codicons": "0.0.20",
		"axios": "^0.26.1",
		"crc-32": "^1.2.2",
		"eslint": "^8.26.0",
		"typescript": "^4.8.4",
		"sqlite3": "^5.1.2"
	}
}
